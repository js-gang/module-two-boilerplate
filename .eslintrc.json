{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "curly": "off",
        "import/no-extraneous-dependencies": "off",
        "import/no-unresolved": "warn",
        "import/extensions": "off",
        "arrow-body-style": ["error", "as-needed"],
        "indent": ["error", 4]
    }
}
