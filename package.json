{
  "name": "module-two-boilerplate",
  "version": "0.0.1",
  "description": "It's a boilerplate for future shiny app",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint",
    "test": "NODE_PATH=./src mocha --compilers js:babel-core/register --require jsdom-global/register --require whatwg-fetch/fetch --recursive tests",
