    "build": "NODE_ENV=production webpack",
    "coverage": "nyc --all --require babel-register --reporter=html --include=src/**/*.js npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-gang/module-two-boilerplate.git"
  },
  "author": "js-gang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/js-gang/module-two-boilerplate/issues"
  },
  "homepage": "https://github.com/js-gang/module-two-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.5.0",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.0.2",
    "mockery": "^2.0.0",
    "nyc": "^8.3.0",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "~1.13.2",
    "webpack-dev-server": "~1.15.1",
    "whatwg-fetch": "^1.0.0"
  }
